<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="upfront.mxitup">

    <configlet
        title="Mxitup Settings"
        action_id="upfront.mxitup.settings"
        appId="upfront.mxitup"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@mxitup-settings"
        icon_expr="string:${portal_url}/++resource++upfront.mxitup/mxitup.gif"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>
